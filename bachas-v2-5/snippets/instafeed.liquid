{{ 'instafeed.min.js' | asset_url | script_tag }}

<!--start Brand logo -->
<div class="section-brand-logo-area">
  <div class="container">
    {% if section.settings.insta_title != blank%}
    <div class="row">
      <div class="col-md-12">
        <div class="section-title">
          <h4><span>{{ section.settings.insta_title }}</span></h4>
        </div>
      </div>
    </div>
    {% endif %}
    <div class="row">
      <div class="col-lg-12 col-md-12 col-sm-12">
        <!-- start brand logo image -->
        <div class="insta-carousel owl-carousel carousel-style-one brand-section" id="instafeed">
        </div>
        <!--End brand logo image -->
      </div>
    </div>
  </div>
</div>
<!-- End Brand logo -->

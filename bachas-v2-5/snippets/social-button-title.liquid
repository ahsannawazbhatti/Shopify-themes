    <a data-toggle="tooltip" data-placement="bottom" href="{{ settings.social_instagram_link }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Instagram' | escape }}">
      <i class="fa fa-instagram"></i>{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Instagram' | escape }}
    </a>
  </li>
  {% endif %}
  {% if settings.social_tumblr_link != blank %}

{% if settings.footer_social_enable %}
<ul class="{% if class %}{{ class }} {% else %} list-inline share-social{% endif %}">
  {% if settings.social_facebook_link != blank %}
  <li>
    <a data-toggle="tooltip" data-placement="bottom" href="{{ settings.social_facebook_link }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Facebook' | escape }}">
      <i class="fa fa-facebook-square"></i>{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Facebook' | escape }}
    </a>
  </li>
  {% endif %}
  {% if settings.social_twitter_link != blank %}
  <li>
    <a data-toggle="tooltip" data-placement="bottom" href="{{ settings.social_twitter_link }}" title="{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Twitter' | escape }}">
      <i class="fa fa-twitter-square"></i>{{ 'layout.footer.store_on_social_network' | t: store_name: shop.name, social_network: 'Twitter' | escape }}
    </a>
  </li>
  {% endif %}
  {% if settings.social_pinterest_link != blank %}
